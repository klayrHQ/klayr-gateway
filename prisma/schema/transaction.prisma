// TODO: recipient, probably when handled all tx events?
model Transaction {
  id            String  @id
  height        Int
  module        String
  command       String
  nonce         String
  fee           String
  minFee        String
  senderAddress String
  sender        Account @relation(fields: [senderAddress], references: [address])
  params        String
  signatures    String // stringifiy
  index         Int
  block         Block   @relation(fields: [height], references: [height])
}
